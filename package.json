{
  "name": "ai-chat",
  "private": true,
  "type": "module",
  "author": "gabrielcaiana",
  "license": "MIT",
  "version": "0.0.0",
  "description": "A simple AI chat application",
  "keywords": [
    "ai",
    "chat",
    "nuxt",
    "vue"
  ],
  "homepage": "https://github.com/gabrielcaiana/ai-chat",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "nuxt:analyze": "npx nuxt analyze",
    "nuxt:cleanup": "npx nuxt cleanup",
    "nuxt:info": "npx nuxt info",
    "nuxt:upgrade": "npx nuxt upgrade",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:ci": "playwright test --config=playwright.config.ci.ts",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.2.0",
    "@nuxt/eslint": "1.2.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^3.3.3",
    "@nuxtjs/mdc": "^0.17.3",
    "ai": "4.1.51",
    "eslint": "^9.31.0",
    "nuxt": "^4.0.3",
    "ollama-ai-provider": "1.2.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod": "^4.1.5"
  },
  "packageManager": "pnpm@8.10.0+sha512.9c2cb83f2b6cf6a25d8c58300bf99197c7ef3be84cf3e766f29054b40b83b42f8aaba6fcc314a9ecf27c00f7ce80a757bb4c608800e7adbe2d29dc5c7056f5be",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.55.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6",
    "wait-on": "^8.0.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
